local player = game.Players.LocalPlayer
local backpack = player.Backpack

-- Check if the third tool exists in the backpack
if #backpack:GetChildren() >= 3 then
    local thirdTool = backpack:GetChildren()[3]

    -- Equip the third tool
    player.Character.Humanoid:EquipTool(thirdTool)
else
    warn("You don't have a third tool in your backpack.")
end
